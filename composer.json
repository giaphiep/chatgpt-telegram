{
    "name": "giaphiep/chatgpt-telegram",
    "description": "Upgrade Laravel Telegram bots with AI-powered ChatGPT. Create engaging, lifelike conversations effortlessly",
    "keywords": [
        "Laravel",
        "Telegram",
        "ChatGPT",
        "AI-powered",
        "Conversational AI",
        "Chatbot dev",
        "NLP",
        "Interactive messaging",
        "Smart convos"
    ],
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "giaphiep",
            "email": "hiep.giapvan@gmail.com"
        }
    ],
    "homepage": "https://github.com/giaphiep/chatgpt-telegram",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "~7|~8|~9|~10"
    },
    "autoload": {
        "psr-4": {
            "GiapHiep\\OpenAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GiapHiep\\OpenAI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GiapHiep\\OpenAI\\ChatGPTServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
